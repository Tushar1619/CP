import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for(int i=0;i<n;i++)
        arr[i]=sc.nextInt();

        double sum =0.0;
        for(int el:arr){
            sum+=(1.0*el)/100.0;
        }
        sum/=n;
        System.out.println(sum*100.0);
    }
}